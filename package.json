  "version": "1.2.7A",
  "homepage": "http://ariatemplates.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/ariatemplates/ariatemplates.git"
  },
  "main": "src/aria/node.js",
  "dependencies": {
    "wrench": "1.3.x",
    "packman": "0.2.x",

<ul>
<% commits.forEach(function (commit) { %>
	<% if (commit.type === "feat") { %>
		<li><strong><%= commit.title %></strong>
			<blockquote>
				<%= commit.messageLines.join(" ") %>
				<br/>
				See <a href="https://github.com/ariatemplates/ariatemplates/issue/<%= commit.issue %>">#<%= commit.issue %></a>
			</blockquote>
		</li>
	<% } %>
<% }) %>
</ul>